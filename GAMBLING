#include <vector>
#include <string>
#include <chrono>
#include <thread>
using namespace std;
int money = 2;
void gambling(){
    while (money > 0){
        srand(time(0));
        vector<string> gamblecore = {"ğŸ’","ğŸ‹","ğŸ’","ğŸ’°","ğŸƒ","ğŸ””","ğŸ° ",};
        int diddylen = gamblecore.size();
